CborComUserConfig = [
  device_id: uint .size 8,
  server_id: uint,
]

CborCanbusConfig = [
  type: uint,
  bus_channel: uint,
  bitrate: uint
]

CborSystemConfig = [
  device_id: uint .size 8,
  hw_version: uint,
  sw_version: uint,
  build_number: uint,

  com_user_refresh_rate_ms: uint,
  com_users: [0*8 CborComUserConfig],
  canbus_configs: [0*8 CborCanbusConfig],

  json_config_checksum: uint
]
