CborCanSignalConfig = [
  start_bit: uint,
  size_bits: uint,
  factor: float32,
  offset: float32,
  name: tstr,
  unit: tstr
]

CborCanMessageConfig = [
  frame_id: uint,
  send_interval_ms: uint,
  script_path: tstr,

  name: tstr,
  message_size: uint,
  signal_configs: [*CborCanSignalConfig]
]

CborCanChannelConfig = [
  type: uint,
  bus_channel: uint,
  bitrate: uint,
  dbc_file_path: tstr,
  message_configs: [*CborCanMessageConfig]
]

CborCanbusConfig = [
  channel_configs: [0*8 CborCanChannelConfig],

  json_config_checksum: uint
]
