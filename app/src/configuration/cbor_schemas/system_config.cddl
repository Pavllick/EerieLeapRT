ComUserConfig = [
  device_id: uint .size 8,
  server_id: uint,
]

CanbusConfig = [
  bus_channel: uint,
  bitrate: uint
]

SystemConfig = [
  device_id: uint .size 8,
  hw_version: uint,
  sw_version: uint,
  build_number: uint,

  com_user_refresh_rate_ms: uint,
  com_users: [0*8 ComUserConfig],
  canbus_configs: [0*8 CanbusConfig],

  sd_json_checksum: uint
]
